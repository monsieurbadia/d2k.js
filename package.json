{
  "name": "d2k",
  "version": "0.0.13",
  "description": "3d object builder based on three.js x babylon.js that will help you to created a scene in an intuitive and functional way.",
  "keywords": [
    "d2k",
    "d2kjs",
    "d2k.js",
    "javascript",
    "3d",
    "3d-object-builder",
    "primitive-builder",
    "glsl",
    "webgl",
    "webgl2",
    "renderer",
    "rendering-engine",
    "game-development",
    "babylon",
    "babylonjs",
    "babylon.js",
    "three",
    "threejs",
    "three.js"
  ],
  "main": "build/d2k.js",
  "repository": "https://github.com/monsieurbadia/d2k.js.git",
  "author": "monsieurbadia <contact@monsieurbadia.com>",
  "license": "MIT",
  "private": false,
  "alias": {
    "=>": "./src/"
  },
  "scripts": {
    "dev:examples": "concurrently \"node functions/index.js\" \"parcel examples/index.html --global d2k\"",
    "prod:build": "parcel build src/index.js --out-dir build/ --out-file d2k.js --experimental-scope-hoisting --global d2k"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "concurrently": "^5.2.0",
    "parcel-bundler": "^1.12.4",
    "u3s": "^0.0.13"
  }
}
