{
  "name": "d2k",
  "version": "0.0.1",
  "description": " javascript micro-layer based on three.js x babylon.js that will help you to used webgl api in a intuitive an functional way.",
  "keywords": [
    "javascript",
    "d2k",
    "webgl",
    "webgl2",
    "mesh",
    "canvas",
    "renderer",
    "loader",
    "rendering engine",
    "primitives builder",
    "game-development"
  ],
  "main": "build/d2k.js",
  "repository": "https://github.com/monsieurbadia/d2k.js.git",
  "author": "monsieurbadia <contact@monsieurbadia.com>",
  "license": "MIT",
  "private": false,
  "alias": {
    "=>": "./src/"
  },
  "scripts": {
    "dev:examples": "concurrently \"node functions/index.js\" \"parcel examples/index.html --global d2k\"",
    "prod:build": "parcel build src/index.js --out-dir build/ --out-file d2k.js --experimental-scope-hoisting --global d2k"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "concurrently": "^5.2.0",
    "jest": "^25.3.0",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "u3s": "^0.0.13"
  }
}
